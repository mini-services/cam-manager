{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps/camera-manager",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/camera-manager/tsconfig.app.json"
  },
  "projects": {
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "camera-manager": {
      "type": "application",
      "root": "apps/camera-manager",
      "entryFile": "main",
      "sourceRoot": "apps/camera-manager/src",
      "compilerOptions": {
        "tsConfigPath": "apps/camera-manager/tsconfig.app.json"
      }
    }
  }
}